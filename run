set -x 

mkdir -p example/output
go build . 

pushd example
protoc foo.proto --go_out=output
protoc foo.proto --plugin=protoc-gen-custom=/Users/lee.avital/code/protobuilder/protobuilder --custom_out=output
popd


ls ./example/output/github.com/leeavital/protobuilder/example/pb
rm -r ./example/pb/
mv ./example/output/github.com/leeavital/protobuilder/example/pb ./example
go build ./example/pb/...
go test ./...
